{
  "expo": {
    "name": "SmartV3",
    "slug": "smartv3-victory",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/smartv3.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/icons/smartv3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/smartv3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.montri24052542.smartv3victory"
    },

    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "kotlinVersion": "1.9.22"
          }
        }
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "367addf9-adbd-4e90-9262-dd24bd3fc5a0"
      },
      "router": {
        "origin": false
      }
    }
  }
}
